

CREATE COMPUTE MODULE HUB_SAP_SWITCH_UpdateError
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE key CHARACTER InputRoot.DataObject.ns:IIB_SE1_TO_SAP_EVENT.ID_EE_PRESTAZIONE_TRACKING;
		UPDATE Database.IIB_SE1_TO_SAP_EVENT AS T
		SET  FLG_ACQUISITO = 'E'
		WHERE T.ID_EE_PRESTAZIONE_TRACKING = key;
		RETURN FALSE;
	END;

END MODULE;
