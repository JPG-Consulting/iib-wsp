

CREATE COMPUTE MODULE SMISTAMENTO_CADE_UpdateFail
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		UPDATE Database.THOR_FAT.CADE_FILE_OUT AS T SET USR_UPDATED = 'IIB - FAILED', TMS_UPDATED = CURRENT_TIMESTAMP WHERE T.ID_FILE_OUTPUT = InputRoot.DataObject.ns:CADE_FILE_OUT.ID_FILE_OUTPUT;
		RETURN FALSE;
	END;

END MODULE;
