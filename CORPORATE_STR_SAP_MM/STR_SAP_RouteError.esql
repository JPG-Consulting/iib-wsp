

CREATE COMPUTE MODULE STR_SAP_RouteError
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		CASE InputRoot.JSON.Data.DB 
			WHEN 'STR' THEN	
				SET OutputRoot = InputRoot;
				PROPAGATE TO TERMINAL 'out1';
				RETURN FALSE;
			WHEN 'STR1B' THEN
				SET OutputRoot = InputRoot;
				PROPAGATE TO TERMINAL 'out2';
				RETURN FALSE;
			END CASE;
		RETURN TRUE;
	END;
END MODULE;
